{
    "name": "sindll/laravel-oauth2-oceanengine",
    "description": "oceanengine OAuth 2.0 Client Provider for The PHP OAuth2-Client",
    "license": "MIT",
    "authors": [
        {
            "name": "王超",
            "email": "tech@staff.sindll.com"
        }
    ],
    "keywords": [
        "oauth",
        "oauth2",
        "client",
        "authorization",
        "authentication",
        "oceanengine",
        "巨量引擎"
    ],
    "require": {
        "php": ">=7.3",
        "league/oauth2-client": "^2.0"
    },
    "require-dev": {
        "ext-json": "*",
        "phpunit/phpunit": "^9.4",
        "mockery/mockery": "~1.3.0",
        "squizlabs/php_codesniffer": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Sindll\\OAuth2\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sindll\\OAuth2\\Client\\Test\\": "tests/src/"
        }
    }
}
